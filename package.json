{
    "name": "git-urgent",
    "displayName": "Git Urgent!",
    "description": "Search `Git Urgent!` in Command Palette. Then you can git add all, commit, push with a single command",
    "version": "0.0.2",
    "publisher": "arafathusayn",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.gitUrgent"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.gitUrgent",
                "title": "Git Urgent!"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
